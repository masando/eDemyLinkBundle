<h1>Link</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ entity.title }}</td>
        </tr>
        <tr>
            <th>Url</th>
            <td>{{ entity.url }}</td>
        </tr>
        <tr>
            <th>Abstract</th>
            <td>{{ entity.abstract }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('edemy_link_index') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('edemy_link_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{# form(delete_form) #}</li>
</ul>
